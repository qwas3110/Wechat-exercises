<view class="header"> 
  <v-episode
    class="episode"
    index="{{classic.index}}"
  />


  <v-like 
    class="like"
    count="{{classic.fav_nums}}"
    like="{{classic.like_status}}"
    bind:like="onLike"
  />
 
</view>

 <v-movie
    img="{{classic.image}}"
    content="{{classic.content}}"
  />